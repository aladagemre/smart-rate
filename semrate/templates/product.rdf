<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:sm="http://www.pythonkursu.com/sm#">
  <rdf:Description rdf:about="http://www.w3.org/">
    <dc:title>{{ product.name}}</dc:title> 
	{% for parameter in parameters %}
	<sm:parameter>
		<sm:parametername>{{parameter.category_parameter.name}}</sm:parametername>
		<sm:parameterrating>{{parameter.get_score}}</sm:parameterrating>
		{% for tag in parameter.tags %}
		<sm:tag>{{tag}}</sm:tag>
		{% endfor %}
	</sm:parameter>
	{% endfor %}
  </rdf:Description>
</rdf:RDF>
  